{
	"name": "@prismicio/vue",
	"version": "3.2.1",
	"description": "Vue plugin, components, and composables to fetch and present Prismic content",
	"keywords": [
		"typescript",
		"prismic",
		"vue",
		"plugin"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-vue.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"vetur"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"playground:build": "vite build playground",
		"playground:dev": "vite playground",
		"prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:dry": "standard-version --dry-run",
		"release:beta": "npm run test && standard-version --release-as major --prerelease beta && git push --follow-tags && npm run build && npm publish --tag beta",
		"release:beta:dry": "standard-version --release-as major --prerelease beta --dry-run",
		"lint": "eslint --ext .js,.ts .",
		"types": "vitest typecheck --run && tsc --noEmit",
		"types:watch": "vitest typecheck",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size"
	},
	"dependencies": {
		"@prismicio/client": "^7.0.1",
		"isomorphic-unfetch": "^3.1.0",
		"vue-router": "^4.2.1"
	},
	"devDependencies": {
		"@prismicio/mock": "^0.2.0",
		"@size-limit/preset-small-lib": "^8.2.4",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/jsdom-global": "^3.0.4",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitest/coverage-c8": "^0.31.1",
		"@vue/compiler-sfc": "^3.3.4",
		"@vue/eslint-config-typescript": "^11.0.3",
		"@vue/test-utils": "^2.3.2",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-vue": "^9.14.0",
		"jsdom": "^22.0.0",
		"jsdom-global": "^3.0.2",
		"prettier": "^2.8.8",
		"prettier-plugin-jsdoc": "^0.4.2",
		"size-limit": "^8.2.4",
		"standard-version": "^9.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.8",
		"vite-plugin-sdk": "^0.1.1",
		"vitest": "^0.31.1",
		"vue": "^3.3.4"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"engines": {
		"node": ">=16.10.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"vetur": {
		"tags": "vetur/tags.json",
		"attributes": "vetur/attributes.json"
	}
}
