{
	"name": "@prismicio/vue",
	"version": "5.1.1",
	"description": "Vue plugin, components, and composables to fetch and present Prismic content",
	"keywords": [
		"typescript",
		"prismic",
		"vue",
		"plugin"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-vue.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"vetur"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"playground:build": "vite build playground",
		"playground:dev": "vite playground",
		"prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:dry": "standard-version --dry-run",
		"release:alpha": "npm run test && standard-version --release-as major --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
		"release:alpha:dry": "standard-version --release-as major --prerelease alpha --dry-run",
		"lint": "eslint",
		"types": "vitest --typecheck.only --run && vue-tsc --noEmit",
		"types:watch": "vitest --typecheck.only",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size"
	},
	"dependencies": {
		"@prismicio/client": "^7.16.1",
		"esm-env": "^1.2.2",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@prismicio/mock": "^0.7.1",
		"@size-limit/preset-small-lib": "^11.1.6",
		"@trivago/prettier-plugin-sort-imports": "^5.2.1",
		"@types/jsdom-global": "^3.0.7",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitest/coverage-v8": "^3.0.2",
		"@vue/eslint-config-typescript": "^14.2.0",
		"@vue/test-utils": "^2.4.6",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-tsdoc": "^0.4.0",
		"eslint-plugin-vue": "^9.32.0",
		"jsdom": "^26.0.0",
		"jsdom-global": "^3.0.2",
		"prettier": "^3.4.2",
		"prettier-plugin-jsdoc": "^1.3.2",
		"size-limit": "^11.1.6",
		"standard-version": "^9.5.0",
		"typescript": "~5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.9",
		"vite-plugin-dts": "^4.5.0",
		"vite-plugin-sdk": "^0.1.3",
		"vitest": "^3.0.2",
		"vue": "^3.5.13",
		"vue-tsc": "^2.2.0"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"engines": {
		"node": ">=16.10.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
